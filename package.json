{
  "name": "functorial-data-migration",
  "version": "0.1.0",
  "description": "Data migration using category theory",
  "scripts": {
    "clean": "rm -rf .cache .spago dist node_modules output",
    "dev": "spago build --purs-args '-g sourcemaps' && spago build --watch --purs-args '-g sourcemaps' > /dev/null | parcel dev assets/index.html",
    "build": "spago bundle-app --main Main && parcel build assets/index.html",
    "test": "spago test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rskew/functorial-data-migration.git"
  },
  "author": "Rowan Skewes",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rskew/functorial-data-migration/issues"
  },
  "homepage": "https://github.com/rskew/functorial-data-migration#README",
  "dependencies": {
    "uuid": "^3.3.3",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "purescript": "^0.13.3",
    "sass": "^1.23.0",
    "spago": "^0.10.0"
  }
}
